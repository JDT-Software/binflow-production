{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd BinFlow.API && dotnet restore && dotnet publish -c Release -o out"
  },
  "deploy": {
    "startCommand": "cd BinFlow.API/out && dotnet BinFlow.API.dll",
    "healthcheckPath": "/health"
  }
}

